<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/android_password"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EC7373"
    >

    <TextView
        android:id="@+id/enter_passw"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="137dp"
        android:layout_marginTop="105dp"
        android:text="@string/enter_passw"
        android:textAppearance="@style/enter_passw" />

    <ImageView
        android:id="@+id/line_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/enter_passw"
        android:layout_marginLeft="80dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/line" />

    <ImageView
        android:id="@+id/rectangle_1"
        android:layout_width="71dp"
        android:layout_height="69dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="220dp"
        android:background="@drawable/rectangle_voter"
        />

    <ImageView
        android:id="@+id/rectangle_2"
        android:layout_width="71dp"
        android:layout_height="69dp"
        android:layout_marginLeft="129dp"
        android:layout_marginTop="220dp"
        android:background="@drawable/rectangle_code" />

    <ImageView
        android:id="@+id/rectangle_3"
        android:layout_width="71dp"
        android:layout_height="69dp"
        android:layout_marginLeft="217dp"
        android:layout_marginTop="220dp"
        android:background="@drawable/rectangle_code" />

    <ImageView
        android:id="@+id/rectangle_4"
        android:layout_width="71dp"
        android:layout_height="69dp"
        android:layout_marginLeft="305dp"
        android:layout_marginTop="220dp"
        android:background="@drawable/rectangle_code" />



    <ImageView
        android:id="@+id/line_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="85dp"
        android:layout_marginTop="348dp"
        android:background="@drawable/line" />


</RelativeLayout>
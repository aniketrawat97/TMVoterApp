<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="123dp"
        android:background="#FFFFFF">


        <TextView
            android:id="@+id/bestroleplayer"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/bestroleplayer"
            android:layout_marginLeft="90dp"
            android:layout_marginTop="33dp"
            android:textAppearance="@style/roleplayers" />

    </LinearLayout>


    <RelativeLayout
        android:id="@+id/relativetmofday"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:background="#5FA0B6">


    <TextView
        android:id="@+id/role"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/toastmaster"
        android:layout_marginTop="35dp"
        android:layout_marginLeft="30dp"
        android:textAppearance="@style/toastmaster"
        />

        <TextView
            android:id="@+id/person"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/role"
            android:text="@string/tm_zakiuddi"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="10dp"
            android:textAppearance="@style/tm_zakiuddi"
            />

    </RelativeLayout>

    <View
        android:id="@+id/line_1"
        android:layout_width="300dp"
        android:layout_height="2dp"
        android:layout_below="@id/relativetmofday"
        android:layout_marginLeft="65dp"
        android:background="#FFFFFF" />


    <RelativeLayout
        android:id="@+id/cardttmaster"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:background="#5FA0B6">


    <TextView
        android:id="@+id/ttmaster"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/ttmaster"
        android:layout_marginTop="35dp"
        android:layout_marginLeft="30dp"
        android:textAppearance="@style/ttmaster"
        />

        <TextView
            android:id="@+id/ttmastername"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/ttmaster"
            android:text="@string/tm_zakiuddi"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="10dp"
            android:textAppearance="@style/tm_zakiuddi"
            />

    </RelativeLayout>


    <View
        android:id="@+id/line_2"
        android:layout_width="300dp"
        android:layout_height="2dp"
        android:layout_below="@id/cardttmaster"
        android:layout_marginLeft="65dp"
        android:background="#FFFFFF" />


    <RelativeLayout
        android:id="@+id/cardgeneralevaluater"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:background="#5FA0B6">


    <TextView
        android:id="@+id/generalevaluater"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/ttmaster"
        android:layout_marginTop="35dp"
        android:layout_marginLeft="30dp"
        android:textAppearance="@style/ttmaster"
        />

        <TextView
            android:id="@+id/genralevaluaterName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/generalevaluater"
            android:text="@string/tm_zakiuddi"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="10dp"
            android:textAppearance="@style/tm_zakiuddi"
            />

    </RelativeLayout>


    <View
        android:id="@+id/line_3"
        android:layout_width="300dp"
        android:layout_height="2dp"
        android:layout_below="@id/cardttmaster"
        android:layout_marginLeft="65dp"
        android:background="#FFFFFF" />

    <RelativeLayout
        android:id="@+id/cardgrammerian"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:background="#5FA0B6">


    <TextView
        android:id="@+id/grammerian"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/ttmaster"
        android:layout_marginTop="35dp"
        android:layout_marginLeft="30dp"
        android:textAppearance="@style/ttmaster"
        />

        <TextView
            android:id="@+id/grammerianname"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/grammerian"
            android:text="@string/tm_zakiuddi"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="10dp"
            android:textAppearance="@style/tm_zakiuddi"
            />

    </RelativeLayout>


</LinearLayout>
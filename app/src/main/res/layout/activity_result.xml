<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="123dp"
        android:background="#FFFFFF">


        <TextView
            android:id="@+id/result"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/result"
            android:layout_marginStart="150dp"
            android:layout_marginTop="33dp"
            android:textAppearance="@style/roleplayers" />

    </LinearLayout>


    <TextView
        android:id="@+id/prepspeaker"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:text="@string/prepspeaker"
        android:background="#EC7373"
        android:textAlignment="center"
        android:textAppearance="@style/prepared_sp" />


    <View
        android:id="@+id/line_1"
        android:layout_width="300dp"
        android:layout_height="2dp"
        android:layout_marginStart="65dp"
        android:background="#FFFFFF" />

    <RelativeLayout
        android:id="@+id/cardone"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:background="#EC7373">


    <TextView
        android:id="@+id/best"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/best"
        android:textAppearance="@style/prepared_sp" />


    <TextView
        android:id="@+id/name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/tm_shurti"
        android:layout_marginStart="115dp"
        android:layout_toEndOf="@id/best"
        android:textAppearance="@style/prepared_sp" />

    <TextView
        android:id="@+id/count"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/count"
        android:layout_marginStart="115dp"
        android:layout_toEndOf="@id/name"
        android:textAppearance="@style/prepared_sp" />


</RelativeLayout>

    <View
        android:id="@+id/line_2"
        android:layout_width="300dp"
        android:layout_height="2dp"
        android:layout_marginStart="65dp"
        android:background="#FFFFFF" />



    <RelativeLayout
        android:id="@+id/cardtwo"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:background="#EC7373">


        <TextView
            android:id="@+id/second"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/second"
            android:textAppearance="@style/prepared_sp" />


        <TextView
            android:id="@+id/namesecond"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/tm_shurti"
            android:layout_marginStart="87dp"
            android:layout_toEndOf="@id/second"
            android:textAppearance="@style/prepared_sp" />

        <TextView
            android:id="@+id/seccount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/count"
            android:layout_marginStart="115dp"
            android:layout_toEndOf="@id/namesecond"
            android:textAppearance="@style/prepared_sp" />


    </RelativeLayout>

    <View
        android:id="@+id/line_3"
        android:layout_width="300dp"
        android:layout_height="2dp"
        android:layout_marginStart="65dp"
        android:background="#FFFFFF" />



    <TextView
        android:id="@+id/ttspeaker"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:text="@string/ttspeaker"
        android:background="#5FA0B6"
        android:textAlignment="center"
        android:textAppearance="@style/prepared_sp" />


    <View
        android:id="@+id/line_4"
        android:layout_width="300dp"
        android:layout_height="2dp"
        android:layout_marginStart="65dp"
        android:background="#FFFFFF" />


    <RelativeLayout
        android:id="@+id/cardthree"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:background="#5FA0B6">


        <TextView
            android:id="@+id/ttbest"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/best"
            android:textAppearance="@style/prepared_sp" />


        <TextView
            android:id="@+id/ttname"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/tm_shurti"
            android:layout_marginStart="115dp"
            android:layout_toEndOf="@id/ttbest"
            android:textAppearance="@style/prepared_sp" />

        <TextView
            android:id="@+id/ttbcount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/count"
            android:layout_marginStart="115dp"
            android:layout_toEndOf="@id/ttname"
            android:textAppearance="@style/prepared_sp" />


    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/cardfour"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:background="#5FA0B6">


        <TextView
            android:id="@+id/ttsecond"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/second"
            android:textAppearance="@style/prepared_sp" />


        <TextView
            android:id="@+id/ttsecname"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/tm_shurti"
            android:layout_marginStart="90dp"
            android:layout_toEndOf="@id/ttsecond"
            android:textAppearance="@style/prepared_sp" />

        <TextView
            android:id="@+id/ttseccount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/count"
            android:layout_marginStart="115dp"
            android:layout_toEndOf="@id/ttsecname"
            android:textAppearance="@style/prepared_sp" />


    </RelativeLayout>





    <View
        android:id="@+id/line_5"
        android:layout_width="300dp"
        android:layout_height="2dp"
        android:layout_marginStart="65dp"
        android:background="#FFFFFF" />


    <RelativeLayout
        android:id="@+id/cardfive"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:background="#5FA0B6">


        <TextView
            android:id="@+id/tthree"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/three"
            android:textAppearance="@style/prepared_sp" />


        <TextView
            android:id="@+id/tthreename"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/tm_shurti"
            android:layout_marginStart="110dp"
            android:layout_toEndOf="@id/tthree"
            android:textAppearance="@style/prepared_sp" />

        <TextView
            android:id="@+id/tthreecount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/count"
            android:layout_marginStart="115dp"
            android:layout_toEndOf="@id/tthreename"
            android:textAppearance="@style/prepared_sp" />


    </RelativeLayout>

    <View
        android:id="@+id/line_7"
        android:layout_width="300dp"
        android:layout_height="2dp"
        android:layout_marginStart="65dp"
        android:background="#FFFFFF" />

    <RelativeLayout
        android:id="@+id/cardrole"
        android:layout_width="match_parent"
        android:layout_height="110dp"
        android:background="#5FA0B6">


        <TextView
            android:id="@+id/roleplayer"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/bestroleplayer"
            android:layout_marginTop="25dp"
            android:layout_marginStart="30dp"
            android:textAppearance="@style/ttmaster"
            />

        <TextView
            android:id="@+id/rolecount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/count"
            android:layout_marginStart="155dp"
            android:layout_marginTop="25dp"
            android:layout_toEndOf="@id/roleplayer"
            android:textAppearance="@style/prepared_sp" />




        <TextView
            android:id="@+id/roleplayername"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/roleplayer"
            android:text="@string/tm_zakiuddi"
            android:layout_marginStart="30dp"
            android:layout_marginTop="10dp"
            android:textAppearance="@style/tm_zakiuddi"
            />

    </RelativeLayout>

    <View
        android:id="@+id/line_8"
        android:layout_width="300dp"
        android:layout_height="2dp"
        android:layout_marginStart="65dp"
        android:background="#FFFFFF" />

    <RelativeLayout
        android:id="@+id/cardauxrole"
        android:layout_width="match_parent"
        android:layout_height="110dp"
        android:background="#EC7373">


        <TextView
            android:id="@+id/auxroleplayer"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/bestauxroleplayers"
            android:layout_marginTop="25dp"
            android:layout_marginStart="30dp"
            android:textAppearance="@style/ttmaster"
            />

        <TextView
            android:id="@+id/roleauxcount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/count"
            android:layout_marginStart="65dp"
            android:layout_marginTop="25dp"
            android:layout_toEndOf="@id/auxroleplayer"
            android:textAppearance="@style/prepared_sp" />




        <TextView
            android:id="@+id/auxroleplayername"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/auxroleplayer"
            android:text="@string/tm_zakiuddi"
            android:layout_marginStart="30dp"
            android:layout_marginTop="10dp"
            android:textAppearance="@style/tm_zakiuddi"
            />

    </RelativeLayout>

    <View
        android:id="@+id/line_9"
        android:layout_width="300dp"
        android:layout_height="2dp"
        android:layout_marginStart="65dp"
        android:background="#FFFFFF" />




    <RelativeLayout
        android:id="@+id/cardeval"
        android:layout_width="match_parent"
        android:layout_height="110dp"
        android:background="#5FA0B6">


    <TextView
        android:id="@+id/evaluater"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/bestevaluater"
        android:layout_marginTop="25dp"
        android:layout_marginStart="30dp"
        android:textAppearance="@style/ttmaster"
        />

    <TextView
        android:id="@+id/evalcount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/count"
        android:layout_marginStart="185dp"
        android:layout_marginTop="25dp"
        android:layout_toEndOf="@id/evaluater"
        android:textAppearance="@style/prepared_sp" />




    <TextView
        android:id="@+id/evaluatername"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/evaluater"
        android:text="@string/tm_zakiuddi"
        android:layout_marginStart="30dp"
        android:layout_marginTop="10dp"
        android:textAppearance="@style/tm_zakiuddi"
        />
    </RelativeLayout>




</LinearLayout>